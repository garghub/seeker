#!/bin/sh

DUALCASE=1
OLD_IFS="$IFS"
nl='
'
for i in "$@" ; do
   IFS=
   case $i in
   -J* )       jopts=$jopts$nl`echo $i | sed -e 's/^-J//'` ;;
   *   )       opts=$opts$nl$i ;;
   esac
   IFS="$OLD_IFS"
done
unset DUALCASE

BASE="`dirname $0`/.."

IFS=$nl
java ${jopts} -Xbootclasspath/p:$BASE/lib/javac.jar -jar $BASE/lib/javac.jar ${opts}
